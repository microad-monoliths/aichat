<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/start_constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/duo_imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:src="@drawable/duo_image"
        app:layout_constraintEnd_toStartOf="@+id/left_vertical"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/orbis_imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:src="@drawable/orbis_image"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <ImageView
        android:id="@+id/decencia_imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="140dp"
        android:layout_marginBottom="50dp"
        android:src="@drawable/decencia_image"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/left_vertical"
        app:layout_constraintHorizontal_bias="0.706"
        app:layout_constraintTop_toTopOf="@+id/orbis_imageView" />

    <ImageView
        android:id="@+id/astalift_imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:src="@drawable/astalift_image_new"
        app:layout_constraintStart_toStartOf="@+id/right_vertical"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/clear_imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="5dp"
        android:src="@drawable/decencia_image_new"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/astalift_imageView" />

    <ImageView
        android:id="@+id/orbis_ud_imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="196dp"
        android:src="@drawable/orbis_ud_image"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="@+id/right_vertical"
        app:layout_constraintTop_toTopOf="@+id/clear_imageView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="110dp"
        android:fontFamily="@font/academy_engraved_std_regular"
        android:letterSpacing="0.15"
        android:text="@string/skincare_check_message_en"
        android:textColor="@color/jp_text_gray_color"
        android:textSize="44sp"
        app:layout_constraintEnd_toStartOf="@+id/right_vertical"
        app:layout_constraintStart_toStartOf="@+id/left_vertical"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="110dp"
        android:fontFamily="@font/noto_sans_jp_regular"
        android:text="@string/skincare_check_message_jp"
        android:textColor="@color/jp_text_gray_color"
        android:textSize="98sp"
        app:layout_constraintEnd_toStartOf="@+id/right_vertical"
        app:layout_constraintStart_toStartOf="@+id/left_vertical"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="184dp"
        android:fontFamily="@font/noto_sans_jp_regular"
        android:text="@string/when_you_answer_7_questions_jp"
        android:textColor="@color/jp_text_gray_color"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/noto_sans_jp_regular"
        android:text="@string/we_find_out_which_skincare_products_are_right_for_you"
        android:textColor="@color/jp_text_gray_color"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7" />

    <Button
        android:id="@+id/startButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="@drawable/start_button_gradient"
        android:drawableEnd="@drawable/arrow_icon"
        android:drawablePadding="100sp"
        android:fontFamily="@font/noto_sans_jp_regular"
        android:paddingStart="200sp"
        android:paddingEnd="50sp"
        android:text="@string/start_diagnosis"
        android:textColor="@android:color/white"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/right_vertical"
        app:layout_constraintStart_toStartOf="@+id/left_vertical"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/left_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.21" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/right_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.79" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="221dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="63dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="689dp" />

</androidx.constraintlayout.widget.ConstraintLayout>